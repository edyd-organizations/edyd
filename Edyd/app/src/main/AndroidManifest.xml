<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oto.edyd" >

    <!--支持多屏-->
    <supports-screens
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:xlargeScreens="true"
        android:anyDensity="true"/>

    <!--系统权限-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--短信服务-->
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <!--拨打电话的权限-->
    <uses-permission android:name="android.permission.CALL_PHONE"></uses-permission>

    <!-- 高德地图-->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <!-- 聚合官网 -->


    <application
        android:name="com.oto.edyd.EdydApplication"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="0dfb6053d90a759dcf7c0a6216e58de5" />
        <!-- 聚合数据 -->
        <meta-data
            android:name="com.thinkland.juheapi.openid"
            android:value="JH5a2427812fb55d29e478171470e4f965" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".SetUpActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".MultyLocationActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".CurrentWeatherReportActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".MarkerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".WeatherActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".AccountTypeActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".AccountInformationActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".MyWayBillActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".OrderDetailActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".ExecutingOrder"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".TransportServiceActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".OrderOperateActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".HistoryTransportOrderActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".LocationActivity"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name="com.mob.tools.MobUIShell"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:windowSoftInputMode="stateHidden|adjustResize" />

        <receiver android:name=".components.SmsReceiver">
            <intent-filter android:priority="800">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <service android:name=".service.WeatherService"></service>
        <service android:name=".service.TimerService"
            android:enabled="true"
            android:process="system"></service>
    </application>
</manifest>
